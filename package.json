    "@types/node": "^22.10.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vueuse/core": "^11.3.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.2",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  },
  "dependencies": {
    "@primevue/themes": "^4.5.4",
    "@vueuse/core": "^11.3.0"
