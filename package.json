{
  "name": "@incoder/kanban",
  "version": "1.0.0",
  "description": "Enterprise-grade Kanban Board component for Vue 3 with PrimeVue integration",
  "type": "module",
  "main": "./dist/incoder-kanban.cjs",
  "module": "./dist/incoder-kanban.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/incoder-kanban.js",
      "require": "./dist/incoder-kanban.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "kanban",
    "kanban-board",
    "drag-and-drop",
    "primevue",
    "component",
    "ui"
